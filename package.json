{
  "name": "verto-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy --entry '/ trade tokens post login gallery app 404 app/all-exchanges app/all-transactions'",
    "validate": "svelte-check",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
