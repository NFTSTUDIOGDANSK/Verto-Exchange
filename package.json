    "eslint": "^7.5.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "sveltejs/prettier-plugin-svelte#5fbbee9",
